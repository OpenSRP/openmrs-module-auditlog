<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.auditlog">

	<class name="MonitoredObject" table="auditlog_monitored_object">
	
		<id name="monitoredObjectId" type="java.lang.Integer" column="monitored_object_id">
			<generator class="native">
				<param name="sequence">monitored_object_monitored_object_id_seq</param>
			</generator>
		</id>
		
		<discriminator column="monitored_object_id" insert="false" />
		
		<property name="className" type="string" column="class_name" length="512" unique="true" not-null="true" update="false" />
		
		<property name="creatorDetails" type="string" column="creator_details" length="512" not-null="true" update="false" />
		
		<property name="dateCreated" type="java.util.Date" column="date_created" length="19" not-null="true" update="false" />
		
		<property name="uuid" type="string" length="38" unique="true" not-null="true" update="false" />
		
	</class>
</hibernate-mapping>
